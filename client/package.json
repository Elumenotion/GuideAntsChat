{
  "name": "guideants",
  "version": "0.6.9",
  "type": "module",
  "description": "Framework-agnostic web component for a published guides chat UI with server-side proxy support",
  "main": "dist/guideants-chat.iife.js",
  "module": "dist/guideants-chat.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/guideants-chat.es.js",
      "require": "./dist/guideants-chat.iife.js"
    },
    "./proxy": {
      "types": "./dist/proxy/index.d.ts",
      "import": "./dist/proxy/index.js",
      "require": "./dist/proxy/index.cjs"
    },
    "./proxy/express": {
      "types": "./dist/proxy/express.d.ts",
      "import": "./dist/proxy/express.js",
      "require": "./dist/proxy/express.cjs"
    }
  },
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "build": "npm run build:browser && npm run build:proxy && npm run copy-types",
    "build:browser": "vite build --config vite.config.ts",
    "build:proxy": "vite build --config vite.proxy.config.ts",
    "copy-types": "node scripts/copy-types.js",
    "preview": "vite preview --config vite.config.ts"
  },
  "keywords": [
    "web-component",
    "custom-elements",
    "conversation",
    "ai-chat",
    "proxy",
    "guideants"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "dougware",
  "license": "MIT",
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "rehype-slug": "^6.0.0",
    "mermaid": "^11.6.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-rehype": "^11.1.1",
    "rehype": "^13.0.1",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^3.7.0"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  }
}


